#include <stdio.h>
#include <stdlib.h>

int main()
{
   float minimo=0,maximo=0,c=0;

    printf("Entre com o limite inferior ");
   scanf("%f",&minimo);
   printf("Entre com o limite superior ");
   scanf("%f",&maximo);
   printf("\e[H\e[2J");//comando para limpar tela


   for(minimo;minimo<=maximo;minimo++)printf("\nConversao de %.2f %.2f  ",minimo, (5*(minimo-32))/9);




    return 0;
}
