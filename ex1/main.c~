#include <stdio.h>
#include <stdlib.h>

int main()
{
  int  numeros [5];
  int i = 0;
  int nun = 0;

    for(i=0;i<5;i++){
       printf("\n Entre com o valor ");
       scanf("%d",&nun);
       numeros[i]=nun;
    }
    system("cls");
    for(i=0;i<5;i++){
        printf("\n valor de %d ao quadrado e %d",numeros[i],numeros[i]*numeros[i]);
    }

    return 0;
}
